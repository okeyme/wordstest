/* pages/yinbiao/pindu.wxss */
.header{background-color: #fed025;}
.pindunav{ border-top: 1px solid #ddd;border-right: 1px solid #ddd;}
.flex-view-item8{	width: 12.5%; text-align: center;}
.flex-view-item8.active{background-color:#fed025;}
.flex-view-item8:first-child {
	border-left: none;
}
.flex-view-item8 text{ width:100%;height:100%; display: inline-block; line-height: 28px; border-left: 1px solid #ddd;border-bottom: 1px solid #ddd; text-decoration: underline;}

.list .flex{border-right: 1px solid #ddd;}
.list .item2{border-left: 1px solid #ddd;border-bottom: 1px solid #ddd; padding:4px 0;}
.list .yb{width:20%; font-weight: bold; line-height: 26px; color:#0ae;}
.list .zm{ width:80%;}
.list .zm text{ padding:0 10px; display: inline-block; line-height: 26px;}