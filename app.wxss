.container{ padding: 10px;}
.container-layout{width:100%;}
.drop {
	position:relative
}
.drop-menu {
	position:absolute;
	display:none;
	top:100%;
	left:0;
	z-index:1;
	float:left;
	min-width:160rpx;
	padding:5rpx 0;
	margin:1rpx 0 0;
	list-style:none;
	background-color:#fff;
	background-clip:padding-box;
	border:1rpx solid #ccc;
	border:1rpx solid rgba(0,0,0,.15);
	border-radius:4rpx;
	-webkit-box-shadow:0 6rpx 12rpx rgba(0,0,0,.175);
	box-shadow:0 6rpx 12rpx rgba(0,0,0,.175)
}
.drop-menu.pull-right {
	right:0;
	left:auto
}
.drop-menu a {
	color:#333;
	padding:5rpx 20rpx;
	display:block;
	clear:both;
	white-space:nowrap;
	transition:all 1s cubic-bezier(0.175,0.885,0.32,1) 0s
}
.drop-menu a:hover,.drop-menu a:focus {
	background:#f5f5f5;
	transition:all 1s cubic-bezier(0.175,0.885,0.32,1) 0s
}
.drop-menu .disabled a {
	color:#999;
	pointer-events:none
}
.drop-menu .disabled a:hover,.drop-menu .disabled a:focus {
	cursor:not-allowed
}
.drop-menu .divider {
	background-color:#ddd;
	height:1rpx;
	overflow:hidden;
	margin:8rpx 0
}
.drop-menu .drop-meun-head {
	display:block;
	padding:5rpx 20rpx;
	font-size:12rpx;
	color:#999
}
.open .drop-menu {
	display:block
}

.txt {
	display:inline-block;
	width:48rpx;
	height:48rpx;
	line-height:48rpx;
	text-align:center;
	background-color:#f5f5f5
}
.txt-border {
	display:inline-block;
	width:48rpx;
	height:48rpx;
	line-height:48rpx;
	border:solid 1rpx #ddd;
	padding:3rpx;
	vertical-align:bottom
}
.txt-border .txt {
	display:block;
	width:100%;
	height:100%;
	line-height:40rpx
}

.txt-border.txt-large {
	padding:5rpx
}
.txt-large {
	width:128rpx;
	height:128rpx;
	line-height:128rpx;
	font-size:36rpx
}
.txt-large .txt {
	width:116rpx;
	height:116rpx;
	line-height:116rpx
}
.txt-large strong {
	font-size:80rpx;
	font-weight:normal
}
.txt-border.txt-big {
	padding:4rpx
}
.txt-big {
	width:64rpx;
	height:64rpx;
	line-height:64rpx;
	font-size:20rpx
}
.txt-big .txt {
	width:54rpx;
	height:54rpx;
	line-height:54rpx
}
.txt-big strong {
	font-size:36rpx;
	font-weight:normal
}
.txt-border.txt-small {
	padding:2rpx
}
.txt-small {
	width:32rpx;
	height:32rpx;
	line-height:32rpx
}
.txt-small .txt {
	width:26rpx;
	height:26rpx;
	line-height:26rpx
}
.txt-border.txt-little {
	padding:1rpx
}
.txt-little {
	width:16rpx;
	height:16rpx;
	line-height:16rpx;
	font-size:12rpx
}
.txt-little .txt {
	width:12rpx;
	height:12rpx;
	line-height:12rpx;
	font-size:12rpx
}

.panel {
	box-sizing: border-box;
	background-color: white; padding: 15px; border-radius: 16px; 
}
.panel-head {
	padding:15px;
	border-bottom:solid 1px #ddd
}
.panel-foot {
	padding:15px;
	border-top:solid 1px #ddd
}
.panel-body {
	padding:15px
}
.panel .bg-main,.panel .bg-sub,.panel .bg-dot,.panel .bg-red,.panel .bg-yellow,.panel .bg-green {
	color:#fff
}
.panel .panel-body+.table,.panel .panel-body+.list-group {
	border-top:solid 1rpx #ddd
}
.panel .list-group {
	border:0;
	border-radius:0
}
.panel-group {
	border-top:solid 1rpx #ddd;
	border-bottom:solid 1rpx #ddd;
	border-left:none;
	border-right:0;
	list-style:none;
	padding:15rpx
}
.tab .tab-more {
	float:right;
	margin:20rpx 0 0 20rpx;
	font-size:12rpx
}
.dialog-mask {
	position:fixed;
	left:0;
	top:0;
	right:0;
	bottom:0;
	background:#000;
	z-index:10;
	filter:alpha(opacity=50);
	opacity:.5;
	display:block;
	overflow-x:hidden;
	overflow-y:auto
}
.dialog {
	border:1rpx solid #ddd;
	box-shadow:0 3rpx 9rpx rgba(0,0,0,0.5);
	border-radius:4rpx;
	background-color:#fff;
	display:none
}
.dialog .dialog-head,.dialog .dialog-foot {
	padding:20rpx 20rpx
}
.dialog .dialog-head {
	border-bottom:solid 1rpx #ddd;
	background-color:#f5f5f5;
	border-radius:4rpx 4rpx 0 0
}
.dialog .dialog-head view {
	font-size:16rpx
}
.dialog .dialog-head .close {
	float:right;
	line-height:24rpx
}
.dialog .dialog-body {
	padding:15rpx 20rpx
}
.dialog .dialog-foot {
	text-align:right;
	border-top:solid 1rpx #ddd
}
.dialog.open {
	display:block
}
.tip {
	display:inline-block;
	background-color:#f5f5f5;
	border:1rpx solid #ddd;
	border-radius:3rpx;
	box-shadow:0 2rpx 10rpx rgba(0,0,0,0.2)
}
.tip .tip-line {
	margin:0;
	padding:2rpx 5rpx;
	font-size:12rpx;
	line-height:16rpx
}
.tip .tip-title {
	padding:2rpx 8rpx
}
.tip .tip-body {
	padding:8rpx;
	background:#fff;
	color:#666;
	border-radius:0 0 3rpx 3rpx;
	font-size:12rpx;
	line-height:20rpx
}
.tip .image {
	border-radius:3rpx
}
.tip-bottom.border-red .arrow {
	border-bottom-color:#f00
}
.tip.bg-main,.tip.bg-sub,.tip.bg-dot,.tip.bg-red,.tip.bg-yellow,.tip.bg-green {
	color:#fff
}
.alert {
	border-radius:5rpx;
	padding:15rpx;
	border:solid 1rpx #dddddd;
	background-color:#f5f5f5
}
.alert .close {
	float:right;
	line-height:18rpx
}

