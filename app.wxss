/*===胶囊按钮组===*/
/* 按钮组容器，设置弹性布局让按钮并排 */
.capsule-group {
  display: flex;
  border-radius: 12px;
  overflow: hidden; /* 确保子元素圆角生效 */
}
.capsule-btn {
	flex-shrink: 0; /* 防止按钮被压缩 */
	white-space: nowrap; /* 防止文字换行 */
	padding:0 15px;
  height: 36px;
  line-height: 36px;
  font-size: 14px;
  text-align: center;
  background-color: #f7f6f6;
  color: #333;
  border: none;
  cursor: pointer;
	transition: background-color 0.2s;
	border: 1px solid #e0e0e0;
}

/* 选中态样式 */
.capsule-btn.active {
  background-color: #2fc6a6;
  color: #fff;
}

/* 中间按钮样式 */
.capsule-btn:not(:first-child):not(:last-child) {
  border-radius: 0;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
}

/* 左右两侧按钮样式 */
.capsule-btn:first-child {
	border-radius: 12px 0 0 12px;
	border-right:none;
}
.capsule-btn:last-child {
	border-radius: 0 12px 12px 0;
	border-left:none;
}
/*===胶囊按钮组===*/
