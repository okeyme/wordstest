.item-options-button{margin:15px 0; font-size: 13px; position: relative; border: 1px solid #e9ecf4; border-radius: 32rpx; padding: 20rpx; background-color: white;}
.item-options-button .flex-item{justify-content: center; 
	align-items: center; }
.item-options-button .flex-item.wordaudio{width:100rpx; flex: none; border-right:1px solid #e9ecf4;}
.item-options-button .wordaudio .image{ width:58rpx;height:48rpx;display:inline-block;background:url(https://w.360e.cn/miniapi/Public/images/sound_black.png) no-repeat center; vertical-align:middle;}
.item-options-button.active .wordaudio.play .image{ background-image:url(https://w.360e.cn/miniapi/Public/images/sound.gif);}

.item-options-button.selected.correct{background-color: #e4fdfa;}
.item-options-button.selected.wrong{background-color: #fcbaba;}
.item-options-button.selected.correct::before {
  content: "";
  position: absolute;
  right: 5px;
  top: 50%; margin-top: -15rpx;
  transform: translateY(-50%);
  width: 24rpx;
  height: 12rpx;
  border-left: 10rpx solid #2fc6a6; /* 对勾左边 */
  border-bottom: 10rpx solid #2fc6a6; /* 对勾下边 */
  transform: rotate(-45deg); /* 旋转形成对勾 */
}
/* 错误样式（叉号） */
.item-options-button.selected.wrong::before,
.item-options-button.selected.wrong::after {
  content: "";
  position: absolute;
  right: 5px;
  top: 50%;
  width: 30rpx; /* 叉号宽度 */
  height: 10rpx; /* 线条粗细 */
  background-color: #dc3545; /* 红色 */
  border-radius: 2rpx; /* 圆角 */
}
.item-options-button.selected.wrong::before {
  transform: translateY(-50%) rotate(45deg); /* 第一条线 */
}
.item-options-button.selected.wrong::after {
  transform: translateY(-50%) rotate(-45deg); /* 第二条线 */
}
